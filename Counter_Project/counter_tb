T_0.0 ;
    %load/vec4 v000001e053e1df00_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v000001e053e16d30_0;
    %addi 1, 0, 4;
    %assign/vec4 v000001e053e16d30_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_000001e053cc4780;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001e053e1e040_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v000001e053e1e040_0;
    %inv;
    %store/vec4 v000001e053e1e040_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_000001e053cc4780;
T_2 ;
    %vpi_call 2 29 "$monitor", "Time = %0t | clk = %b | reset = %b | enable = %b | count = %b", $time, v000001e053e1e040_0, v000001e053cd2b50_0, v000001e053cd2ab0_0, v000001e053e1e0e0_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001e053cd2b50_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001e053cd2ab0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001e053cd2b50_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001e053cd2ab0_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001e053cd2ab0_0, 0, 1;
